# Application
APP_NAME=AbleToCompete MVP
VERSION=1.0.0
DEBUG=True

# Database
DATABASE_URL=postgresql+asyncpg://user:pass@localhost:5432/abletocompete

# Redis
REDIS_URL=redis://localhost:6379

# Security
SECRET_KEY=change-this-to-a-random-secret-key-in-production
